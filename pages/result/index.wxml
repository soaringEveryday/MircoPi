<!--pages/result/index.wxml-->
<view class='container-result'>
    <image class='pic' src='{{pic}}'></image>
    <text class='name'>{{name}}</text>
    <text class='result'>{{resultText}}</text>

    <wxc-button style="margin-top:50rpx;" size="large" type="beauty" value="{{btnText}}" btnStyle="background: #FF3D4E" bindtap='doFinish'>
    </wxc-button>
</view>
